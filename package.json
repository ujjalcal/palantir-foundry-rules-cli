{
  "name": "@title-review-app/foundry-rules-cli",
  "version": "1.2.0",
  "description": "Config-driven CLI and library for managing Foundry Rules proposals",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/proposal-cli-v2.d.ts",
      "import": "./dist/proposal-cli-v2.js"
    }
  },
  "bin": {
    "foundry-rules": "dist/bin/cli.js"
  },
  "files": [
    "dist",
    "config",
    "prompts",
    "samples"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "cli": "node dist/bin/cli.js",
    "cli:dev": "npx tsx src/bin/cli.ts",
    "validate": "node dist/bin/cli.js --config config/demo-product.json validate",
    "templates": "node dist/bin/cli.js --config config/demo-product.json template --list"
  },
  "dependencies": {
    "@osdk/client": "^2.6.0",
    "@osdk/foundry": "^2.44.0",
    "@osdk/oauth": "^1.6.0",
    "lz-string": "^1.5.0"
  },
  "devDependencies": {
    "@types/lz-string": "^1.5.0",
    "@types/node": "^20.19.27",
    "ajv": "^8.17.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "registry": "https://ujjal-test.usw-23.palantirfoundry.com/artifacts/api/repositories/ri.artifacts.main.repository.b31057c1-a6f7-4e10-803c-9a20fa108856/contents/release/npm/"
  }
}
